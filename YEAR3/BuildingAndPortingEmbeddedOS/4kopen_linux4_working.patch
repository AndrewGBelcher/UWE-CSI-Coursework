+			status = "okay";
+			clock-frequency = <300000000>;
+			reg = <0x8C85000 0xFFF>,
+				<0x6000000 0x3FFFF>;
+			reg-names = "hva_registers", "hva_esram";
+			interrupts = <0 58 0>, <0 59 0>;
+			interrupt-names = "hva_its_irq", "hva_err_irq";
