+	of_node_put(np);
+
+	for (i = 0; i < no_pctl; i++) {
+		struct sti_ddr3_low_power_info *temp = &ddr_sys_conf[i];
+
+		if (!temp->sysconf_base)
+			goto err_1;
+		/*
