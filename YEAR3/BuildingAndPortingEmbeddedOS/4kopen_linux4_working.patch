+int sti_cps_setup(struct sti_hw_state_desc *state,
+		  unsigned long *ddr_pctl_addr,
+		  unsigned int no_pctl,
+		  struct sti_ddr3_low_power_info *ddr_sys_conf,
+		  bool ddr_sr_ack_pctl)
