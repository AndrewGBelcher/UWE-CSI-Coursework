+/**
+ * enum st_lpm_config_reboot_type - Define DDR state while booting
+ * @ST_LPM_REBOOT_WITH_DDR_SELF_REFRESH
+ * @ST_LPM_REBOOT_WITH_DDR_OFF
+ */
+enum st_lpm_config_reboot_type {
